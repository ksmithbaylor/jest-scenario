    "scenarios",
    "outline",
    "outlines",
    "cucumber",
    "test",
    "tests",
    "testing"
  ],
  "author": "Kevin Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithbaylor/tape-scenario/issues"
  },
  "homepage": "https://github.com/ksmithbaylor/tape-scenario#readme",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.11.6",
    "eslint": "^3.3.1",
    "eslint-config-airbnb-base": "^5.0.2",
    "eslint-plugin-import": "^1.13.0",
    "tap-difflet": "^0.6.0",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "intercept-stdout": "^0.1.2"
