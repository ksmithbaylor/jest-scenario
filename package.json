{
  "name": "tape-scenario",
  "version": "0.2.4",
  "description": "A small helper for reducing test verbosity",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint *.js",
    "build": "babel src --out-dir dist",
    "test": "tape -r babel-register tests | tap-difflet"
